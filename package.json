{
  "name": "clone-tabnews",
  "version": "1.0.0",
  "description": "Projeto para estudo com base no site https://www.tabnews.com.br/",
  "main": "index.js",
  "scripts": {
    "dev": "npm run service:up && next dev",
    "service:up": "docker compose -f infra/compose.yaml up -d",
    "service:stop": "docker compose -f infra/compose.yaml stop",
    "service:down": "docker compose -f infra/compose.yaml down -v",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "next": "^13.1.6",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "prettier": "^2.8.8"
  }
}
